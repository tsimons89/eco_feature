#include "Filter_set.hpp"
Filter_set::Filter_set(Genome max_genome){
  set_max(max_genome);
  
}

void Filter_set::set_max(Genome max_genome){
  max_x_blur = max_genome.max_x_blur;
  max_y_blur = max_genome.max_y_blur;
  max_x_diff = max_genome.max_x_diff;
  max_y_diff = max_genome.max_y_diff;  
}


